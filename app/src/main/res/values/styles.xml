<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="Theme.Cabinet" parent="@style/CabinetParent">
        <item name="android:windowBackground">@android:color/holo_green_dark</item>
    </style>

    <style name="Theme.CabinetStatusTranslucent" parent="@style/Theme.Cabinet">
        <item name="android:windowBackground">@android:color/holo_purple</item>
    </style>

    <style name="Theme.CabinetNavTranslucent" parent="@style/Theme.Cabinet">
        <item name="android:windowBackground">@android:color/holo_orange_dark</item>
    </style>

    <style name="Theme.CabinetNavStatusTranslucent" parent="@style/Theme.Cabinet">
        <item name="android:windowBackground">@android:color/holo_blue_dark</item>
    </style>


    <style name="Theme.CabinetDark" parent="@style/CabinetDarkParent" />

    <style name="Theme.CabinetDarkStatusTranslucent" parent="@style/Theme.CabinetDark" />

    <style name="Theme.CabinetDarkNavTranslucent" parent="@style/Theme.CabinetDark" />

    <style name="Theme.CabinetDarkNavStatusTranslucent" parent="@style/Theme.CabinetDark" />


    <style name="Theme.CabinetTrueBlack" parent="@style/CabinetTrueBlackParent" />

    <style name="Theme.CabinetTrueBlackStatusTranslucent" parent="@style/Theme.CabinetTrueBlack" />

    <style name="Theme.CabinetTrueBlackNavTranslucent" parent="@style/Theme.CabinetTrueBlack" />

    <style name="Theme.CabinetTrueBlackNavStatusTranslucent" parent="@style/Theme.CabinetTrueBlack" />


    <style name="ButtonCabinet" parent="android:Widget.Holo.Light.Button">
        <item name="android:background">@drawable/rect_button_selector</item>
        <item name="android:textColor">#fff</item>
        <item name="android:minHeight">48dp</item>
        <item name="android:minWidth">64dp</item>
    </style>

    <style name="ButtonCabinetDark" parent="android:Widget.Holo.Light.Button">
        <item name="android:background">@drawable/rect_button_selector_dark</item>
        <item name="android:textColor">#fff</item>
        <item name="android:minHeight">48dp</item>
        <item name="android:minWidth">64dp</item>
    </style>

</resources>